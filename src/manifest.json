{
  "name": "Auto Picture-in-Picture For Netflix",
  "description": "Auto Picture In Picture Extension For Netflix",
  "version": "1.0",
  "icons": {
    "128": "assets/icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "19": "assets/icon19.png",
      "38": "assets/icon38.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "tabs"
  ],
  "minimum_chrome_version": "69.0.3483.0",
  "manifest_version": 2
}